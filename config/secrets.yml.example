  facebook_secret: <%= ENV["FACEBOOK_SECRET"] %>
  google_oauth2_key: <%= ENV["GOOGLE_KEY"] %>
  google_oauth2_secret: <%= ENV["GOOGLE_SECRET"] %>
  openam_username: <%= ENV["OPENAM_USERNAME"] %>
  openam_password: <%= ENV["OPENAM_PASSWORD"] %>
  saml_sp_entity_id: <%= ENV["SAML_SP_ENTITY_ID"] || "consul" %>
  saml_idp_metadata_url: <%= ENV["SAML_IDP_METADATA_URL"] %>
  saml_idp_sso_service_url: <%= ENV["SAML_IDP_SSO_SERVICE_URL"] || "http://localhost:8081/openam/idpssoinit?metaAlias=/idp&spEntityID=consul&binding=HTTP-POST" %>
