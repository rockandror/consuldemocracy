<%= back_link_to admin_settings_path %>

<% if feature?(:smtp_configuration) %>
  <h2><%= t("admin.settings.index.smtp.title") %></h2>
  <%= render "mixed_settings_table", settings: @settings %>
<% else %>
  <div class="callout primary">
    <%= t("admin.settings.index.smtp.how_to_enable") %>
  </div>
<% end %>
